p = women$height
q = women$weight
print(p)
print(q)
plot(p, q, 
     main = "Women", 
     xlab = "Height", 
     ylab = "Weight", 
     pch = 19,  
     frame = FALSE) 


p = c(1,12,45,46,47,38,29,40)
q = c(10,15,20,25,30,35,40,45)
cor(p,q)
plot(p, q, 
     main = "Women", 
     xlab = "Height", 
     ylab = "Weight", 
     pch = 19,  
     frame = FALSE) 


p = c(100,90,80,70,60,50,40)
