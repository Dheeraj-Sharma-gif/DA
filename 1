     main = "Women", 
     xlab = "Height", 
     ylab = "Weight", 
     pch = 19, 
     frame = FALSE)


#ggplot
x = 1:20
y = sample(1000, 20, replace = TRUE)
df = data.frame(x, y)

library(ggplot2)

ggplot(df, aes(x, y, group = 1)) + 
  geom_boxplot()


#positive corelation
head(mtcars)
x = mtcars$cyl
y = mtcars$carb
cor(x,y)
plot(x,y, main = "Mtcars", xlab ="cyl", ylab = "carb", pch =19, frame =FALSE)

#negative corelation
head(mtcars)
x = mtcars$cyl
y = mtcars$mpg
cor(x,y)
plot(x,y, main = "Mtcars", xlab ="cyl", ylab = "mpg", pch =19, frame =FALSE)
